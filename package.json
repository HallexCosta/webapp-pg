{
  "name": "webapp-pg",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./dist/server.js",
    "install:ci": "yarn install --frozen-lock",
    "build": "tsc",
    "start:development:watch": "cross-env NODE_ENV=development nodemon -L",
    "start:development:cluster": "ts-node -r dotenv/config --transpile-only ./src/cluster.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/pg": "^8.6.5",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "pg": "^8.7.3"
  }
}
